{
  "name": "@graphwiz/hub-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:docker": "vite --host --port 5173",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:prod": "playwright test --config=playwright.config.prod.ts",
    "test:e2e:prod:ui": "playwright test --config=playwright.config.prod.ts --ui",
    "test:e2e:prod:headed": "playwright test --config=playwright.config.prod.ts --headed",
    "check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@graphwiz/protocol": "workspace:*",
    "@graphwiz/types": "workspace:*",
    "@graphwiz/ui-kit": "workspace:*",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "^2.16.0",
    "cannon-es": "^0.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "jsdom": "^23.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  }
}
